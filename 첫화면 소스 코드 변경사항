void titleDraw(void) {

	printf("                *                                  \n"); // 맨위에 4칸 개행  
	printf("        @@@@     *     @     @  @@@@   @@@@  @@@@  \n");
	printf("        @   @   @@@@   @@   @@  @   @  @     @   @ \n");
	printf("        @@@@   @    @  @ @ @ @  @@@@   @@@@  @@@@  \n");
	printf("        @   @  @    @  @  @  @  @   @  @     @  @  \n");
	printf("        @@@@    @@@@   @     @  @@@@   @@@@  @   @ \n");
	printf("\n");
	printf("               @     @      @      @   @ \n");
	printf("               @@   @@     @ @     @@  @ \n");
	printf("               @ @ @ @    @@@@@    @ @ @ \n");
	printf("               @  @  @   @     @   @  @@ \n");
}
// 이인구 학우분이 올린 디자인 살짝 변경해봤는데 이상한거 같네요.
